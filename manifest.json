{
  "manifest_version": 3,
  "name": "New Tab Opener",
  "version": "1.0",
  "description": "Opens a new tab with a click of the extension icon",
  "permissions": ["tabs"],
  "action": {
    "default_icon": {
      "16": "assets/icon-light-16.png",
      "48": "assets/icon-light-48.png",
      "128": "assets/icon-light-128.png"
    }
  },
  "icons": {
    "16": "assets/icon-light-16.png",
    "48": "assets/icon-light-48.png",
    "128": "assets/icon-light-128.png"
  },
  "background": {
    "service_worker": "background.js"
  }
}